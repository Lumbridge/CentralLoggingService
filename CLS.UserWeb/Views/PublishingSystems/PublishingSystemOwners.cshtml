
@model List<CLS.Core.Data.PublishingSystemOwner>

@{
    ViewBag.Title = "Publishing System Owners";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@if (Model.Count <= 0)
{
    <h2>An error occurred.</h2>
}
else
{
    <h2>Publishing System Owners for @Model.First().PublishingSystem.Name</h2>

    <div class="row">
        <div id="PublishingSystemOwnersTableContainer" class="col-lg-12">
            @Html.Partial("_PublishingSystemOwnersTable", Model)
        </div>
    </div>
}